{
	"name": "backend",
	"version": "1.0.0",
	"description": "stdlib developer dashboard",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev nodemon src/index.ts",
		"build": "tsc",
		"lint": "npx @biomejs/biome lint --write .",
		"format": "npx @biomejs/biome format --write .",
		"check": "npx @biomejs/biome check --write .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"prepare": "husky"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Prakash-Raut/Developer-Dashboard.git"
	},
	"keywords": [
		"metrics",
		"failure",
		"logging"
	],
	"author": "Prakash Raut",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Prakash-Raut/Developer-Dashboard/issues"
	},
	"homepage": "https://github.com/Prakash-Raut/Developer-Dashboard#readme",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/express": "^5.0.0",
		"@types/node": "^22.13.9",
		"husky": "^9.1.7",
		"lint-staged": "^15.4.3",
		"nodemon": "^3.1.9",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2"
	},
	"lint-staged": {
		"*.ts": [
			"npm run check"
		]
	},
	"dependencies": {
		"cross-env": "^7.0.3",
		"dotenv-flow": "^4.1.0",
		"express": "^4.21.2",
		"winston": "^3.17.0"
	}
}
